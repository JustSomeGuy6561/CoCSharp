<UserControl x:Class="CoCWinDesktop.CustomControls.SideBar"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:CoCWinDesktop.CustomControls"
			 xmlns:SBV="clr-namespace:CoCWinDesktop.CustomControls.SideBarViews"
			 xmlns:SBMV="clr-namespace:CoCWinDesktop.CustomControls.SideBarModelViews"
			 Name="_this">
	<UserControl.Resources>
		<DataTemplate DataType="{x:Type SBMV:StandardSideBarModelView}">
			<SBV:StandardSideBarView/>
		</DataTemplate>
	</UserControl.Resources>
	<ContentControl Width="195" Height="590" FocusVisualStyle="{x:Null}" Content="{Binding SideBarObject, ElementName=_this, Mode=OneWay}"/>
</UserControl>
